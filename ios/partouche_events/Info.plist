	<array>
		<string>DMSans-Black.ttf</string>
		<string>DMSans-Bold.ttf</string>
		<string>DMSans-Light.ttf</string>
		<string>DMSans-Medium.ttf</string>
		<string>DMSans-Regular.ttf</string>
		<string>DMSans-SemiBold.ttf</string>
	</array>
